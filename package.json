{
  "name": "dictionary-api",
  "version": "1.0.0",
  "description": "Dictionary API ",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src --exec babel-node src --presets es2015",
    "build": "babel src -s -D -d dist --presets es2015",
    "start": "NODE_ENV=production node dist",
    "test": "NODE_ENV=test eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyndseybrowning/dictionary-api.git"
  },
  "keywords": [
    "Dictionary",
    "API",
    "Node",
    "Express",
    "JavaScript"
  ],
  "author": "Lyndsey Browning",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lyndseybrowning/dictionary-api/issues"
  },
  "homepage": "https://github.com/lyndseybrowning/dictionary-api#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.13.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "express-validator": "^3.1.2"
  }
}
